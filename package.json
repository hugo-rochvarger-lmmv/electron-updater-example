{
  "name": "electron-updater-example",
  "version": "0.8.8",
  "main": "main.js",
  "description": "electron-updater example project",
  "author": "Matt Haggard",
  "scripts": {
    "build": "electron-builder --mac -p always"
  },
  "devDependencies": {
    "electron": "^29.1.0",
    "electron-builder": "^24.12.0",
    "electron-notarize": "^1.1.1"
  },
  "dependencies": {
    "electron-log": "^5.1.1",
    "electron-updater": "^6.1.8"
  },
  "build": {
    "appId": "com.github.hugo-rochvarger-lmmv.electronupdaterexample",
    "publish": [{
      "provider": "generic",
      "url": "http://localhost:8080/myapplicationname/test/${os}",
      "publishAutoUpdate": true,
      "channel":"latest"
  }],
    "afterSign": "./build/notarize.js",
    "mac": {
      "category": "your.app.category.type"
    },
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false
    },
    "linux": {
      "category": "your.app.category.type",
      "target": "AppImage"
    }
  }
}
